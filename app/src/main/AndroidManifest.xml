<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.exercise1">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Exercise1">
        <receiver
            android:name=".Receiver.MyAlarmReceiver"
            android:enabled="true"
            android:exported="true"></receiver>

        <service android:name=".Services.DownloadService" />
        <service android:name=".Services.MessageService" />

        <activity android:name=".DownloadActivity" />

        <receiver android:name=".Receiver.ReceiveReply" />

        <activity android:name=".OtherActivity" />
        <activity android:name=".ImageActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="com.example.Broadcastd" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>